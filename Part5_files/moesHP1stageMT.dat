/*---------------------------------------------------------------------------------------------------------------------------------------
Set the maximum scaling factor
---------------------------------------------------------------------------------------------------------------------------------------*/
let  Fmax['HP1stageMT'] := 10;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the minimum temperature of the HP
---------------------------------------------------------------------------------------------------------------------------------------*/
let Tminheating['HP1stageMT'] := 65;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set flow rate of electricity as a function of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
for{t in Time}{
	let Flowin_hp["Electricity", "HP1stageMT", t] := Qheatingsupply["HP1stageMT"]/COP_HP1stageMT[t];
}
/*---------------------------------------------------------------------------------------------------------------------------------------
Set cost parameter
---------------------------------------------------------------------------------------------------------------------------------------*/
param years_HPMT default 20;
param IC_HPMT default 0;
param F_BMHPMT := 2.7;

let IC_HPMT := interest*(IC_HPMT+1)^years_HPMT/((interest+1)^years_HPMT-1);

let cinv1['HP1stageMT'] := 0.98 * IC_HPMT * 1.9230e+04 * F_BMHPMT * (I_2019/I_2001);				#  [CHF]

let cinv2['HP1stageMT'] := 0.98 * IC_HPMT * 293.1891 * 343.1697 * F_BMHPMT * (I_2019/I_2001);			# continious investment cost [CHF/kW]
