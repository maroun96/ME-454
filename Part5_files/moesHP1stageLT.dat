/*---------------------------------------------------------------------------------------------------------------------------------------
Set the maximum scaling factor
---------------------------------------------------------------------------------------------------------------------------------------*/
let  Fmax['HP1stageLT'] := 10;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the minimum temperature of the HP
---------------------------------------------------------------------------------------------------------------------------------------*/
let Tminheating['HP1stageLT'] := 50;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set main function
---------------------------------------------------------------------------------------------------------------------------------------*/
param N_HPLT default 20;
for{t in Time}{
	let Flowin_hp["Electricity", "HP1stageLT", t] := Qheatingsupply["HP1stageLT"] / COP_HP1stageLT[t];
}

/*---------------------------------------------------------------------------------------------------------------------------------------
Set cost parameter
---------------------------------------------------------------------------------------------------------------------------------------*/
param years_HPLT default 20;
param IC_HPLT default 0;
param F_BMHPLT := 2.7;

let IC_HPLT := interest*(interest+1)^years_HPLT/((interest+1)^years_HPLT-1);

let cinv1['HP1stageLT'] := 0.98 * IC_HPLT * 1.4028e+04 * F_BMHPLT * (I_2019/I_2001);				#  [CHF]

let cinv2['HP1stageLT'] := 0.98 * IC_HPLT * 310.0724 * 275.7520 * F_BMHPLT * (I_2019/I_2001);			# continious investment cost [CHF/kW]
