/*---------------------------------------------------------------------------------------------------------------------------------------
Set the efficiency of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
param eff_CarnotLT default 0.55;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the minimum temperature of the boiler
---------------------------------------------------------------------------------------------------------------------------------------*/
let Tminheating['HP1stageLT'] := Theating['LowT'] + dTmin;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the sink temperature of the HP
---------------------------------------------------------------------------------------------------------------------------------------*/
param T_sinkLT := Theating['LowT'] + dTmin;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set real COP of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
param COP_HP1stageLT{t in Time}:= (eff_CarnotLT*T_sinkLT)/(T_sinkLT-T_source[t]);
/*---------------------------------------------------------------------------------------------------------------------------------------
Set flow rate of electricity as a function of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
subject to HPLT_elec{t in Time}:
	Flowin['Electricity','HP1stageLT'] * mult_t['HP1stageLT',t] = Qheatingsupply['HP1stageLT'] / COP_HP1stageLT[t];
