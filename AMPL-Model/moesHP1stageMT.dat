/*---------------------------------------------------------------------------------------------------------------------------------------
Set the maximum scaling factor
---------------------------------------------------------------------------------------------------------------------------------------*/
let  Fmax['HP1stageMT'] := 10;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set the minimum temperature of the HP
---------------------------------------------------------------------------------------------------------------------------------------*/
let Tminheating['HP1stageMT'] := 65;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set flow rate of electricity as a function of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
for{t in Time}{
	let Flowin_hp["Electricity", "HP1stageMT", t] := Qheatingsupply["HP1stageMT"]/COP_HP1stageMT[t];
}
/*---------------------------------------------------------------------------------------------------------------------------------------
Set cost parameter
---------------------------------------------------------------------------------------------------------------------------------------*/
param N_HPMT default 20;
param IC_HPMT default 0;

let IC_HPMT := interest*(IC_HPMT+1)^N_HPMT/((interest+1)^N_HPMT-1);

let cinv1['HP1stageMT'] := 26635.96*IC_HPMT;				#  [CHF]

let cinv2['HP1stageMT'] := 53.3408*refSize*IC_HPMT;			# continious investment cost [CHF/kW]
