/*---------------------------------------------------------------------------------------------------------------------------------------
Set the sink temperature of the HP
---------------------------------------------------------------------------------------------------------------------------------------*/
param T_sinkMT := 65+273+5;
/*---------------------------------------------------------------------------------------------------------------------------------------
Set real COP of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
param COP_HP1stageMT{t in Time}:= (eff_Carnot*T_sinkMT/(T_sinkMT - T_source[t]));
/*---------------------------------------------------------------------------------------------------------------------------------------
Set flow rate of electricity as a function of HP
---------------------------------------------------------------------------------------------------------------------------------------*/
for{t in Time}{
	let Flowin_hp["Electricity", "HP1stageMT", t] := Qheatingsupply["HP1stageMT"]/COP_HP1stageMT[t];
}
